/*Layout погода валюта - таблица справа*/
.rate {
  display: inline-block;
  position: relative;
  float: right;}

.rate_title {
	padding-right: 10px;
  width: auto;
}

.rate_back {
  display:inline-block;
  background-color: rgba(255, 255, 255, 0.8);
  -webkit-box-shadow: 0 8px 8px -4px rgba(0, 0, 0, 0.16);
  -moz-box-shadow: 0 8px 8px -4px rgba(0, 0, 0, 0.16);
  box-shadow: 0 8px 8px -4px rgba(0, 0, 0, 0.16);
  padding: 18px 20px 0 20px;
}

.temp {
  font-size: 12px;
  font-weight: 700;
	padding-left: 6px;
}

.rate table, .rate tbody {
  vertical-align: middle;
  margin: 0;
	width: 100%;
}

.rate table:first-child {
  border-bottom: 1px solid rgba(153, 153, 153, 1);
  margin-bottom: 11px;
}

.rate_body {
  width: auto;
}

.rate table th {
	font-weight: 300;
  color: rgba(153, 153, 153, 1);
}

.rate table td, .rate table th {
  padding: 0 0 14px 0;
  line-height: 16px;
  text-align: center;
	font-size: 12px;
}

.rate table td span {
  /*padding-right: 10px;*/
}

.usd_down {
  color: rgba(153, 0, 0, 1);
}

.usd_up {
  color: rgba(0, 153, 0, 1);
}
