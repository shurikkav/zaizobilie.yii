/*Layout мобильный 320px поиск*/
.search_mobile {
  display: inline-block;
  position: relative;
  float: right;
  width: calc(100% - 40px);
  right: 0;
}

.search_mobile form {
  margin: 0;
  padding: 20px;
  line-height: 0;
}

.search_mobile form .search_text {
  display: inline-block;
  position: relative;
  float: left;
  margin: 0;
  width: calc(100% - 30px);
  background: rgba(255, 255, 255, 0.6);;
  border: 0 solid;
  padding-left: 10px;
  line-height: 20px;
}

.search_mobile form .search_image {
  display: inline-block;
  position: relative;
  float: left;
  top: 0;
	margin: 0;
}

.search_mobile. form search_image_catalog, .search_mobile form .search_image {
  margin: 0;
  width: 20px;
  height: 20px;
  background-image: url(../img/Icon_search.svg);
  background-repeat: no-repeat;
  background-position: center;
  border: none;
}
/*Layout мобильный 320px поиск*/

.search {
  display: inline-block;
  position: relative;
  width: calc(25% - 38px);
  height: 20px;
  padding-left: 20px;
  z-index: 1;
}

.search form {
  width: 100%;
  margin: 0;
  padding: 0;
  line-height: 0;
}

.search input[type=text] {
  font-size: 14px;
  line-height: 20px;
	/*background: rgba(191, 199, 205, 1.0);*/
	background: rgba(255, 255, 255, 0.7);
}


.search_text {
  display: inline-block;
  position: relative;
  float: left;
  margin: 0;
  width: calc(100% - 30px);
  background: rgba(240, 240, 240, 1.0);
  border: 0 solid;
}

.search_image {
  display: inline-block;
  position: absolute;
  right: 0;
  top: 20px;
}

.search_image_catalog, .search_image {
  margin: 0 20px 0 0;
  width: 20px;
  height: 20px;
  background: url('../img/search.svg') no-repeat;
  background-position: center center;
  background-size: cover;
  border: none;
}

input[type=submit].search_image:focus {
  background-image: url(../img/search.svg);
  background-repeat: no-repeat;
  background-position: center center;
  background-size: cover;
  -webkit-box-shadow: 0 0 10px 0 rgba(0, 0, 0, 0.4);
  -moz-box-shadow: 0 0 10px 0 rgba(0, 0, 0, 4);
  box-shadow: 0 0 10px 0 rgba(0, 0, 0, 0.4);
}

input[type=submit].search_image:hover {
  -webkit-box-shadow: 0 0 10px 0 rgba(0, 0, 0, 0.4);
  -moz-box-shadow: 0 0 10px 0 rgba(0, 0, 0, 4);
  box-shadow: 0 0 10px 0 rgba(0, 0, 0, 0.4);
}

.footer_search .search {
  width: 100%;
}

.footer_search .search input[type=text] {
  line-height: 18px;
  float: right;
}

